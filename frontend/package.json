        "@types/node": "25.2.2",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.4",
        "@vitest/coverage-v8": "^4.0.18",
        "@vitest/ui": "^4.0.18",
        "babel-plugin-react-compiler": "^1.0.0",
        "cross-env": "^10.1.0",
        "jsdom": "^28.0.0",
        "msw": "^2.12.10",
        "oxfmt": "^0.32.0",
        "oxlint": "^1.47.0",
        "socket.io": "^4.8.3",
        "typescript": "^5.9.3",
        "vite": "^7.3.1",
        "vitest": "^4.0.18",
        "web-vitals": "^5.1.0",
        "wrangler": "^4.65.0"
