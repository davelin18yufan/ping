{
  "name": "ping-web",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--import=./instrument.server.mjs vite dev --port 3000",
    "build": "vite build && cp instrument.server.mjs dist/server",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:file": "vitest run",
    "test:coverage": "vitest run --coverage",
    "deploy": "npm run build && wrangler deploy",
    "start": "node --import ./.output/server/instrument.server.mjs .output/server/index.mjs",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "tsc": "tsc --noEmit 2>&1"
  },
  "dependencies": {
    "@apollo/client": "^4.1.1",
    "@apollo/client-integration-tanstack-start": "^0.14.4-rc.0",
    "@cloudflare/vite-plugin": "^1.21.1",
    "@sentry/tanstackstart-react": "^10.35.0",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.153.2",
    "@tanstack/react-router-devtools": "^1.153.2",
    "@tanstack/react-router-ssr-query": "^1.153.2",
    "@tanstack/react-start": "^1.154.0",
    "@tanstack/react-store": "^0.8.0",
    "@tanstack/router-plugin": "^1.153.2",
    "@tanstack/store": "^0.8.0",
    "better-auth": "^1.4.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.10.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rxjs": "^7.8.2",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.3.6",
    "vite-tsconfig-paths": "^6.0.2"
  },
  "devDependencies": {
    "@tanstack/devtools-event-client": "^0.4.0",
    "@tanstack/devtools-vite": "^0.4.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^10.1.0",
    "jsdom": "^27.0.0",
    "msw": "^2.12.7",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "socket.io": "^4.8.3",
    "typescript": "^5.7.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.59.3"
  },
  "packageManager": "pnpm@10.28.1"
}
